{
  "name": "gensoucycle-portfolio-and-circle-website",
  "description": "Gensobunya's doujin circle and portfolio website",
  "version": "1.0.0",
  "private": true,
  "author": "gensobunya",
  "keywords": [
    "gatsby, frontend, cycle"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby clean && gatsby develop --host=0.0.0.0",
    "serve": "npm run build && gatsby serve",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.14",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "gatsby": "^2.21.33",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-emotion": "^4.3.1",
    "gatsby-plugin-google-tagmanager": "^2.3.3",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-lodash": "^3.3.1",
    "gatsby-plugin-material-ui": "^2.1.8",
    "gatsby-plugin-preconnect": "^1.1.43",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.8",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.7",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-responsive-iframe": "^2.4.2",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-filesystem": "^2.3.7",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^2.8.12",
    "gatsby-transformer-sharp": "^2.5.2",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.6.3",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "typescript": "^3.9.2"
