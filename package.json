{
  "name": "gensoucycle-portfolio-and-circle-website",
  "description": "Gensobunya's doujin circle and portfolio website",
  "version": "1.0.0",
  "private": true,
  "author": "gensobunya",
  "keywords": [
    "Touhou project",
    "cycle",
    "Doujin"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
